<%- model_class = Municipio -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:postos_pesquisados) %>:</strong></dt>
  <dd><%= @municipio.postos_pesquisados %></dd>
  <dt><strong><%= model_class.human_attribute_name(:consumidor_preco_medio) %>:</strong></dt>
  <dd><%= @municipio.consumidor_preco_medio %></dd>
  <dt><strong><%= model_class.human_attribute_name(:consumidor_desvio_padrao) %>:</strong></dt>
  <dd><%= @municipio.consumidor_desvio_padrao %></dd>
  <dt><strong><%= model_class.human_attribute_name(:consumidor_preco_minimo) %>:</strong></dt>
  <dd><%= @municipio.consumidor_preco_minimo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:consumidor_preco_maximo) %>:</strong></dt>
  <dd><%= @municipio.consumidor_preco_maximo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:distribuidor_margem_media) %>:</strong></dt>
  <dd><%= @municipio.distribuidor_margem_media %></dd>
  <dt><strong><%= model_class.human_attribute_name(:distribuidor_preco_medio) %>:</strong></dt>
  <dd><%= @municipio.distribuidor_preco_medio %></dd>
  <dt><strong><%= model_class.human_attribute_name(:distribuidor_desvio_padrao) %>:</strong></dt>
  <dd><%= @municipio.distribuidor_desvio_padrao %></dd>
  <dt><strong><%= model_class.human_attribute_name(:distribuidor_preco_minimo) %>:</strong></dt>
  <dd><%= @municipio.distribuidor_preco_minimo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:distribuidor_preco_maximo) %>:</strong></dt>
  <dd><%= @municipio.distribuidor_preco_maximo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nome) %>:</strong></dt>
  <dd><%= @municipio.nome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:semana_inicio) %>:</strong></dt>
  <dd><%= @municipio.semana_inicio %></dd>
  <dt><strong><%= model_class.human_attribute_name(:semana_fim) %>:</strong></dt>
  <dd><%= @municipio.semana_fim %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              municipios_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_municipio_path(@municipio), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              municipio_path(@municipio),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
