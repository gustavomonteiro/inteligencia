<%= simple_form_for @municipio, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :postos_pesquisados %>
  <%= f.input :consumidor_preco_medio %>
  <%= f.input :consumidor_desvio_padrao %>
  <%= f.input :consumidor_preco_minimo %>
  <%= f.input :consumidor_preco_maximo %>
  <%= f.input :distribuidor_margem_media %>
  <%= f.input :distribuidor_preco_medio %>
  <%= f.input :distribuidor_desvio_padrao %>
  <%= f.input :distribuidor_preco_minimo %>
  <%= f.input :distribuidor_preco_maximo %>
  <%= f.input :nome %>
  <%= f.input :semana_inicio %>
  <%= f.input :semana_fim %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                municipios_path, :class => 'btn' %>
  </div>
<% end %>
