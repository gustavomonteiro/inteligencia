<div class="row-fluid">
	<div class="span6">
		<h4>Bairro com maior média</h4>
		<table class="table table-striped table-bordered">
		  <thead>
			<th>Bairro</th>
			<th>Média de venda</th>
		  </thead>
		  <tbody>
		    <% @bairro_maior_media.each do |week_collection| %>
		      <tr>
		        <td><%= week_collection.bairro %></td>
		        <td><%= week_collection.avg.to_f.round(3) %></td>
		      </tr>
		    <% end %>
		  </tbody>
		</table>		
	</div>
	<div class="span6">
		<h4>Bandeira com maior média</h4>
		<table class="table table-striped table-bordered">
		  <thead>
			<th>Bandeira</th>
			<th>Média de venda</th>
		  </thead>
		  <tbody>
		    <% @bandeira_maior_media.each do |week_collection| %>
		      <tr>
		        <td><%= week_collection.distribuidora %></td>
		        <td><%= week_collection.avg.to_f.round(3) %></td>
		      </tr>
		    <% end %>
		  </tbody>
		</table>		
	</div>
</div>
<div class="row-fluid">
	<div class="span6">
